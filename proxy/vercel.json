{
  "version": 2,
  "functions": {
    "api/gemini-proxy.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/proxy.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/feedback.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "api/gemini-proxy",
      "dest": "/api/gemini-proxy.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "api/proxy",
      "dest": "/api/proxy.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "api/feedback",
      "dest": "/api/feedback.js",
      "methods": ["POST", "OPTIONS"]
    }
  ]
}